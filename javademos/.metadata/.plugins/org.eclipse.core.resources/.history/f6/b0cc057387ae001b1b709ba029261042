package clasessWithAttributes;

public class Product {

	// attribute & field
	private int id;
	private String name;
	private String description;
	private double price;
	private int stockAmount;
	private String renk;
	private String kod;
	
	public int getId() {
		return id;
	}
	
	public void setId(int id) {
		id=id;
	}
	
}
